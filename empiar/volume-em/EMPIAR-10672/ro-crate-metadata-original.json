        {
            "@id": "_:Semiautomatic organelle segmentation",
            "@type": [
                "bia:AnnotationMethod"
            ],
            "title": "Semiautomatic organelle segmentation",
            "protocolDescription": "Segmentation and 3D reconstruction performed using 3D Slicer software with supervised semiautomatic pixel classification mode (3-10 slices automatically segmented for each ROI). Each region colored using paint tools and threshold adjustment. Model maker module used to generate 3D models exported in STL format. Models cleaned and decimated in MeshLab. Metrics for volumes, surface area, and minimum distance between meshes computed using Numpy-STL and trimesh Python packages.",
            "annotationCriteria": null,
            "annotationCoverage": null,
            "transformationDescription": null,
            "spatialInformation": null,
            "methodType": [
                "semi-automated segmentation"
            ],
            "annotationSourceIndicator": null
        },
