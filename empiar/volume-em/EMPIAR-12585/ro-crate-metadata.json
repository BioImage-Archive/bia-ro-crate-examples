        {
            "@id": "_:SBF-SEM tissue preparation with OTO staining",
            "@type": [
                "bia:SpecimenImagingPreparationProtocol"
            ],
            "title": "SBF-SEM tissue preparation with OTO staining",
            "protocolDescription": "Retina samples were fixed in 2.5% glutardialdehyde in 0.1 M cacodylate buffer with 3% sucrose overnight. OTO staining was performed: samples were treated with 2% OsO4 + 1.5% potassium ferrocyanide on ice (90 min), thiocarbohydrazide solution at RT (20 min), 2% OsO4 at RT (30 min), 1% uranyl acetate overnight, and 0.02 M lead aspartate at 60C (30 min). Samples were dehydrated in ascending ethanol and acetone series, embedded in EMS hard plus resin 812, polymerized for 48 h at 60C, mounted on aluminum stubs with conductive glue, and trimmed to 500 um cubes with a diamond knife. Prior to 3D-EM investigation, mesa-preparations were sputtered with 20 nm gold.",
            "signalChannelInformation": []
        },
        {
            "@id": "_:Apreo Volume Scope SBF-SEM acquisition",
            "@type": [
                "bia:ImageAcquisitionProtocol"
            ],
            "title": "Apreo Volume Scope SBF-SEM acquisition",
            "protocolDescription": "Serial block-face scanning electron microscopy was performed using the Apreo Volume Scope (Thermo Fisher Scientific) with integrated microtome for alternating diamond knife cutting and SEM scanning. Scanning parameters: high vacuum mode, working distance 6.5 mm, 1.78 kV, 0.1 nA, 1-3 us pixel dwell time, T1 backscattering electron detector. Scan 1 was acquired in radial orientation at 25 x 25 x 50 nm voxel size, covering from cone outer segments to the scleral border of the inner plexiform layer.",
            "imagingInstrumentDescription": "Apreo Volume Scope (Thermo Fisher Scientific Inc., Brno, Czech Republic)",
            "imagingMethodName": [
                "serial block-face scanning electron microscopy"
            ],
            "fbbiId": [
                "obo:FBbi_00000586"
            ]
