        {
            "@id": "_:High pressure freezing and freeze substitution for FIB-SEM",
            "@type": [
                "bia:SpecimenImagingPreparationProtocol"
            ],
            "title": "High pressure freezing and freeze substitution for FIB-SEM",
            "protocolDescription": "Cultures were centrifuged (2000 g, 2 min) and cryofixed using high-pressure freezing (HPM100, 210 MPa, -196C). Freeze substitution (EM ASF2) replaced vitrified ice with dried acetone plus 2% osmium tetroxide; 0.5% uranyl acetate was added for FIB-SEM. Samples were embedded in Araldite resin and stored dry prior to imaging.",
            "signalChannelInformation": []
        },
        {
            "@id": "_:FIB-SEM tomography (Zeiss CrossBeam 550)",
            "@type": [
                "bia:ImageAcquisitionProtocol"
            ],
            "title": "FIB-SEM tomography (Zeiss CrossBeam 550)",
            "protocolDescription": "FIB-SEM tomography used a Zeiss CrossBeam 550. Resin blocks were mounted on stubs, surface-abraded with a diamond knife, and coated with 4-8 nm platinum; an additional 1-2 um platinum layer was deposited in situ. Serial slicing with a Ga+ ion beam (~700 pA at 30 kV) alternated with SEM imaging at 1.5 kV and ~1 nA using an in-column EsB backscatter detector. Slice thickness was 6-8 nm with matching pixel size for isotropic voxels.",
            "imagingInstrumentDescription": "Zeiss CrossBeam 550 (Zeiss, Germany)",
            "imagingMethodName": [
                "focused ion beam scanning electron microscopy"
            ],
            "fbbiId": [
                "obo:FBbi_00000585"
            ]
        },
